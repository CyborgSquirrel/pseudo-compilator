	scrie 100
altfel
	scrie 69
repeta
	a <- a+1
pana cand a=15
scrie a
b<-5
c <-  (a+2)*b+3
scrie a%5
a <- a-1
scrie a
a<->b
cat timp a > 0 executa
	a <- a-1
	scrie a
scrie 123
pentru i<-1,5,2 executa
	scrie i
